{
    "manifest_version": 2,
    "name": "Imperial converter",
    "version": "1.2",
  
    "description": "Converts all imperial measurements to normal measurements",
  
    "icons": {
      "48": "icons/convert-48.png", 
      "96": "icons/convert-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["convertimperial.js"]
      }
    ]
  }
  